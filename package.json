    "lucide-vue-next": "^0.552.0",
    "nuxt": "^4.2.0",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@nuxt/content": "^3.8.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@vitest/ui": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "happy-dom": "^20.0.10",
